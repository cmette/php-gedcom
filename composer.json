{
    "name": "cmette/php-gedcom",
    "description": "A GEDCOM file parser (read + write) for PHP 8.0+",
    "type": "library",
    "keywords": ["gedcom","parser"],
    "homepage": "http://github.com/cmette/php-gedcom",
    "license": "MIT",
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "squizlabs/php_codesniffer": "3.6.*",
        "rector/rector": "*",
        "driftingly/rector-laravel": "^0.25.0"
    },
    "autoload": {
        "psr-4": {
            "Gedcom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GedcomTest\\": "tests/"
        }
    }
}
